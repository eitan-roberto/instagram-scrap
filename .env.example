# Target Profile to scrape (full URL)
IG_TARGET=https://www.instagram.com/example_profile/

# Max posts to scrape (default: all found)
IG_MAX_POSTS=20

# Scroll rounds to load posts (default: 5)
IG_SCROLL_ROUNDS=5

# Headless mode (default: false to show browser for manual login)
# Set to 'true' only if running in Docker or server without display
IG_HEADLESS=false
